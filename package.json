{
  "name": "fractal6",
  "version": "1.0.0",
  "description": "Organisation for Humans",
  "main": "public/index.html",
  "scripts": {
    "start": "npm run build && npm run dev",
    "build": "npm run build:elm-spa && npm run build:elm",
    "build:elm-spa": "elm-spa build .",
    "build:elm": "elm make src/Main.elm --output dist/elm.js",
    "dev_old": "npm run build && concurrently --raw --kill-others \"npm run js_build:watch\" \"npm run css_build:watch\" \"npm run dev:elm-spa\" \"npm run dev:elm\"",
    "dev:elm-spa": "chokidar src/Pages -c \"npm run build:elm-spa\"",
    "dev:elm": "elm-live src/Main.elm -u -d public -- --debug --output dist/elm.js",
    "mybuild": "npm run css_build && npm run js_build",
    "css_build:watch": "chokidar assets/sass/ -c \"npm run css_build\"",
    "js_build:watch": "chokidar assets/js -c \"npm run js_build\"",
    "css_build": "for k in dark; do node-sass --omit-source-map-url assets/sass/$k.scss dist/$k.css; done",
    "js_build": "cp -v assets/js/*.js dist/",
    "graphql_build": "./node_modules/.bin/elm-graphql --schema-file ./schema.gql --base Fractal --output ./src",
    "webdev": "webpack-dev-server --hot --port 8000 --mode development",
    "webprod": "webpack-dev-server --hot --port 8000 --mode production",
    "prod": "webpack --mode production",
    "dev": "webpack --mode development"
  },
  "keywords": [
    "graphql",
    "elm",
    "fractal",
    "organisation",
    "d3",
    "spa"
  ],
  "author": "Adrien Dulac",
  "license": "GPLv2",
  "dependencies": {
    "@bulvar/bulma": "^0.11.12",
    "@creativebulma/bulma-badge": "^1.0.1",
    "@creativebulma/bulma-tagsinput": "^1.0.3",
    "@creativebulma/bulma-tooltip": "^1.2.0",
    "bulma": "^0.9.4",
    "bulma-modal-fx": "^1.1.1",
    "bulma-switch": "^2.0.4",
    "d3": "^7.6.1",
    "d3-scale": "^4.0.2",
    "elm": "0.19.1-5",
    "minisearch": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@dillonkearns/elm-graphql": "^4.3.0",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.5",
    "chokidar-cli": "3.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "7.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.1.0",
    "elm-hot-webpack-loader": "^1.1.8",
    "elm-live": "4.0.2",
    "elm-spa": "4.1.0",
    "elm-webpack-loader": "^8.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "node-sass": "^7.0.3",
    "postcss-loader": "^7.0.1",
    "postcss-safe-parser": "^6.0.0",
    "postcss-scss": "^4.0.5",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "uglify-js": "^3.17.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  }
}
